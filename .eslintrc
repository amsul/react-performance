{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "globals": {
    "requestAnimationFrame": true,
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["error", {
      "args": "none"
    }],
  },
}
